<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MathTestResult">

    <data>
        <variable
            name="viewModelPr"
            type="com.example.myproject.model.OrderViewModel" />
    </data>
    <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mthresults">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="355dp"
                android:layout_height="143dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/rectangle_35"
                app:layout_constraintBottom_toTopOf="@+id/textView47"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.285"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="335dp"
                android:layout_height="48dp"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="68dp"
                android:fontFamily="@font/secondfont"
                android:text="Ваш результат:"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView23"
                app:layout_constraintStart_toStartOf="@+id/imageView23" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="60dp"
                android:layout_height="61dp"
                android:fontFamily="@font/secondfont"
                android:text="@{viewModelPr.answer.toString()}"
                android:textAlignment="textStart"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintStart_toStartOf="@+id/textView23"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="78dp"
                android:layout_height="61dp"
                android:layout_marginStart="72dp"
                android:fontFamily="@font/secondfont"
                android:text="@{viewModelPr.count.toString()}"
                android:textAlignment="textStart"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <TextView
                android:id="@+id/textView45"
                android:layout_width="28dp"
                android:layout_height="61dp"
                android:layout_marginStart="48dp"
                android:fontFamily="@font/secondfont"
                android:text="/"
                android:textAlignment="textStart"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFFFFF"
                android:src="@drawable/rectangle_27"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/secondfont"
                android:text="Выйти"
                android:textColor="#000000"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/imageButton2"
                app:layout_constraintHorizontal_bias="0.482"
                app:layout_constraintStart_toStartOf="@+id/imageButton2"
                app:layout_constraintTop_toTopOf="@+id/imageButton2" />

            <TextView
                android:id="@+id/textView44"
                android:layout_width="342dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/secondfont"
                android:text="@{viewModelPr.stroke}"
                android:textColor="#FFFFFF"
                android:textSize="44sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView47"
                android:layout_width="232dp"
                android:layout_height="265dp"
                android:layout_marginBottom="76dp"
                android:fontFamily="@font/secondfont"
                android:text="Вы можете сохранить свои результаты и посмотреть их позже в разделе “Мои результаты”"
                android:textColor="#FFFFFF"
                android:textSize="28sp"
                app:layout_constraintBottom_toTopOf="@+id/textView43"
                app:layout_constraintStart_toStartOf="@+id/imageView23" />

            <ImageButton
                android:id="@+id/imageButton12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:backgroundTint="#00FFFFFF"
                android:src="@drawable/rectangle_27"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageButton2" />

            <TextView
                android:id="@+id/textView49"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/secondfont"
                android:text="Сохранить"
                android:textColor="#000000"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/imageButton12"
                app:layout_constraintStart_toStartOf="@+id/imageButton12"
                app:layout_constraintTop_toTopOf="@+id/imageButton12" />


        </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
</layout>