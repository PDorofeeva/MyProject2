<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MathTestResult">

    <data>
        <variable
            name="viewModelPr"
            type="com.example.myproject.model.OrderViewModel" />
    </data>
    <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mathres">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView23"
            android:layout_width="283dp"
            android:layout_height="183dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/secondfont"
            android:text="Хорошая работа! Ваш результат:"
            android:textColor="#FFFFFF"
            android:textSize="44sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="60dp"
            android:layout_height="61dp"
            android:fontFamily="@font/secondfont"
            android:text="@{viewModelPr.answer.toString()}"
            android:textAlignment="textStart"
            android:textColor="#FFFFFF"
            android:textSize="44sp"
            app:layout_constraintStart_toStartOf="@+id/textView23"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/rectangle_7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="78dp"
            android:layout_height="61dp"
            android:layout_marginStart="72dp"
            android:fontFamily="@font/secondfont"
            android:text="@{viewModelPr.count.toString()}"
            android:textAlignment="textStart"
            android:textColor="#FFFFFF"
            android:textSize="44sp"
            app:layout_constraintStart_toStartOf="@+id/textView25"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="28dp"
            android:layout_height="61dp"
            android:layout_marginStart="48dp"
            android:fontFamily="@font/secondfont"
            android:text="/"
            android:textAlignment="textStart"
            android:textColor="#FFFFFF"
            android:textSize="44sp"
            app:layout_constraintStart_toStartOf="@+id/textView25"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <TextView
            android:id="@+id/textView43"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/secondfont"
            android:text="Выйти"
            android:textColor="#000000"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="@+id/imageButton2"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="@+id/imageButton2"
            app:layout_constraintTop_toTopOf="@+id/imageButton2" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
</layout>